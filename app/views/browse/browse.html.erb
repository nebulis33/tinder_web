<div class="container">
    <div class="row">
        <div class="col-3 col-md-4">
        <%= render 'layouts/profile_bar' %>
        
            <ul class="nav nav-tinder-sidebar mb-3" id="pills-tab" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="pills-matches-tab" data-toggle="pill" href="#pills-matches" role="tab" aria-controls="pills-matches" aria-selected="true">Matches</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="pills-messages-tab" data-toggle="pill" href="#pills-messages" role="tab" aria-controls="pills-messages" aria-selected="false">Messages</a>
                </li>
            </ul>
            <div class="tab-content" id="pills-tabContent">
                <div class="tab-pane fade show active" id="pills-matches" role="tabpanel" aria-labelledby="pills-matches-tab">
                    <%= render partial: "browse/matches" %>
                </div>
                <div class="tab-pane fade" id="pills-messages" role="tabpanel" aria-labelledby="pills-messages-tab">
                    <div class="col">
                    <%= render partial: 'list' %>
                    </div>
                </div>
            </div>

        </div>
        <%= render partial: "browse/cards" %>
    </div>
</div>

